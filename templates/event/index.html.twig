{% extends 'base.html.twig' %}

{% block title %}Hello EventController!{% endblock %}

{% block body %}
    {% for event in events %}
        <li>
            {{ event.name }}
        </li>
        <li>
            {{ event.startAt|date('d/m/Y') }}
        </li>
{#        <li>#}
{#            {{ event.duration }}#}
{#        </li>#}
        <li>
            {{ event.registrationEndsAt|date('d/m/Y') }}
        </li>
        <li>
            {{ event.maxUsers }}
        </li>
        <li>
            {{ event.description }}
        </li>
        <li>
            {{ event.location.street }}
        </li>
{#        <li>#}
{#            {{ event.users }}#}
{#        </li>#}
        <li>
           {{ event.status.label }}
        </li>

    {% endfor %}

{% endblock %}
